{
  "name": "kiwicast",
  "title": "kiwicast",
  "description": "Call kiwi commands with raycast",
  "icon": "command-icon.png",
  "author": "matt",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Branch",
      "subtitle": "",
      "description": "Create a branch file for a specific project",
      "mode": "view",
      "preferences": [
        {
          "name": "projectPath",
          "type": "textfield",
          "required": true,
          "title": "Project Path",
          "description": "Path to your kiwi project"
        },
        {
          "name": "kiwiPath",
          "type": "textfield",
          "required": false,
          "title": "Path to kiwi",
          "description": "Path to kiwi executable to run",
          "placeholder": "kiwi stored in PATH"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
